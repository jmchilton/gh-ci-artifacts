{
  "name": "artifact-detective",
  "version": "0.1.0",
  "description": "Detect and parse CI artifact types for test frameworks and linters",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "ci",
    "artifacts",
    "testing",
    "playwright",
    "jest",
    "pytest",
    "junit",
    "eslint",
    "linter",
    "parser"
  ],
  "author": "John Chilton",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmchilton/gh-ci-aritfacts.git",
    "directory": "packages/artifact-detective"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^4.0.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6",
    "yaml": "^2.8.1"
  },
  "dependencies": {
    "cheerio": "^1.1.2"
  }
}
