{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["node", "@playwright/test"]
  },
  "include": ["src/**/*", "test/**/*"],
  "exclude": ["node_modules"]
}
