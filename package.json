{
  "name": "gh-ci-artifacts",
  "version": "1.13.0",
  "description": "Download and parse GitHub Actions CI artifacts and logs for LLM analysis",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "gh-ci-artifacts": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "tsc --noEmit",
    "docs:api": "typedoc --out docs/api/typedoc src/index.ts",
    "docs:dev": "docsify serve docs",
    "docs:build": "npm run docs:api",
    "prepublishOnly": "npm run build && npm test",
    "release": "semantic-release"
  },
  "keywords": [
    "github",
    "actions",
    "ci",
    "artifacts",
    "testing",
    "llm",
    "claude",
    "playwright",
    "jest"
  ],
  "author": "John Chilton",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmchilton/gh-ci-artifacts.git"
  },
  "bugs": {
    "url": "https://github.com/jmchilton/gh-ci-artifacts/issues"
  },
  "homepage": "https://github.com/jmchilton/gh-ci-artifacts#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@vitest/coverage-v8": "^4.0.6",
    "docsify-cli": "^4.4.4",
    "prettier": "3.6.2",
    "semantic-release": "^24.1.2",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.14",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@types/node": "^24.9.2",
    "artifact-detective": "^1.15.0",
    "cheerio": "^1.1.2",
    "commander": "^14.0.2",
    "junit-xml-parser": "^0.0.3",
    "typescript": "^5.9.3",
    "yaml": "^2.8.1"
  }
}
